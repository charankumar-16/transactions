#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/Unscrambl/node_modules/.pnpm/csv-parser@3.0.0/node_modules/csv-parser/bin/node_modules:/home/workspace/Unscrambl/node_modules/.pnpm/csv-parser@3.0.0/node_modules/csv-parser/node_modules:/home/workspace/Unscrambl/node_modules/.pnpm/csv-parser@3.0.0/node_modules:/home/workspace/Unscrambl/node_modules/.pnpm/node_modules:/home/workspace/Unscrambl/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/Unscrambl/node_modules/.pnpm/csv-parser@3.0.0/node_modules/csv-parser/bin/node_modules:/home/workspace/Unscrambl/node_modules/.pnpm/csv-parser@3.0.0/node_modules/csv-parser/node_modules:/home/workspace/Unscrambl/node_modules/.pnpm/csv-parser@3.0.0/node_modules:/home/workspace/Unscrambl/node_modules/.pnpm/node_modules:/home/workspace/Unscrambl/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/csv-parser" "$@"
else
  exec node  "$basedir/../../bin/csv-parser" "$@"
fi
